{
  "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {"regenerator": true}],
    ["@babel/plugin-proposal-decorators", {"legacy": true}],
    ["@babel/plugin-proposal-class-properties", {"loose": true}],
    "babel-plugin-parameter-decorator"
  ]
}
