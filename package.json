{
  "name": "@qcloud-apaas/mobile-ui",
  "version": "0.2.3",
  "description": "Tencent Cloud aPaaS Mobile UI Components",
  "keywords": [
    "react",
    "ui",
    "vant"
  ],
  "author": "Gary Wang",
  "license": "MIT",
  "homepage": "https://github.com/qcloud-apaas/mobile-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qcloud-apaas/mobile-ui.git"
  },
  "bugs": {
    "url": "https://github.com/qcloud-apaas/mobile-ui/issues"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --passWithNoTests --coverage",
    "build:lib": "tsc -d -m commonjs --outDir lib --sourceMap false --inlineSources false",
    "build:es": "tsc -d -m esNext --outDir es --sourceMap false --inlineSources false",
    "build:clean": "rm -rf lib/demos/ es/demos/ lib/*/demo/ es/*/demo/",
    "build": "rm -rf lib es && npm run build:lib && npm run build:es && npm run scss && npm run assets && npm run build:clean",
    "start": "webpack serve",
    "deploy": "NODE_ENV=production webpack && gh-pages -t -d docs",
    "scss": "gulp scss && gulp lib && node-sass src --output lib && node-sass src --output es",
    "assets": "cp -R src/assets/ es/assets/ && cp -R src/assets/ lib/assets/",
    "tsc-check": "tsc --outDir .tsc-check/",
    "eslint": "eslint --ext ts,tsx src",
    "eslint:fix": "eslint --ext ts,tsx src --fix",
    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "stylelint src/**/*.scss --fix",
    "release": "standard-version",
    "pant:local": "rm -rf pant-react && mkdir pant-react && cp -R ../../webyom/pant-react/es/ ./pant-react/es/ && echo \"\n@import '../../../src/styles/pant-react-var.scss';\" >> ./pant-react/es/styles/var.scss",
    "pant": "rm -rf pant-react && mkdir pant-react && cp -R ./node_modules/pant-react/es/ ./pant-react/es/ && echo \"\n@import '../../../src/styles/pant-react-var.scss';\" >> ./pant-react/es/styles/var.scss"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.10",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@vant/touch-emulator": "^1.2.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "commitizen": "^4.1.2",
    "css-loader": "^3.4.2",
    "cssnano": "^5.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "file-loader": "^5.1.0",
    "final-form": "^4.20.2",
    "gh-pages": "^2.2.0",
    "gulp": "^4.0.2",
    "gulp-postcss": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "postcss": "^8.2.14",
    "prettier": "^2.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-final-form": "^6.5.3",
    "react-refresh": "^0.10.0",
    "react-router-dom": "^5.2.0",
    "require-all": "^2.0.0",
    "sass-loader": "^8.0.2",
    "standard-version": "^9.0.0",
    "style-loader": "^1.1.3",
    "stylelint": "^12.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-scss": "^3.13.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.2.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.26.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "closest": "0.0.1",
    "clsx": "^1.1.0",
    "pant-react": "^0.2.3"
  }
}
